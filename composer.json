{
    "name": "spatie/laravel-disable-floc",
    "description": "Automatically disable Google's floc in Laravel apps",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-disable-floc"
    ],
    "homepage": "https://github.com/spatie/laravel-topics-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        },
        {
            "name": "Bruno Bernard",
            "email": "contact.brunobernard@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/contracts": "^7.30|^8.37|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.15|^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DisableFloc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DisableFloc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DisableTopicsApi\\DisableTopicsApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
